public class MaisDois implements Series {
    private int inicio;
    private int valor;

    public MaisDois(){
        this.inicio = 0;
        this.valor = 0;
    }

    public int getNext(){
        valor += 2;
        return valor;
    }

    public void reset(){
        valor = inicio;
    }

    public void setStart(int x){
        inicio = x;
        valor = x;
    }
}
